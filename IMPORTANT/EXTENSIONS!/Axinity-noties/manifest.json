{
  "manifest_version": 3,
  "name": "Axinity Noties",
  "version": "1.0",
  "description": "A beautiful word storage extension",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon/icon16.png",
      "48": "icon/icon48.png",
      "128": "icon/icon128.png"
    }
  },
  "permissions": ["storage", "unlimitedStorage"],
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  }
}