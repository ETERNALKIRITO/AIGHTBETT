{
    "manifest_version": 3,
    "name": "QuickXins",
    "version": "1.0",
    "description": "Beautiful shortcut manager for quick website access",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
      }
    },
    "permissions": ["storage"],
    "icons": {
      "16": "icon/icon16.png",
      "48": "icon/icon48.png",
      "128": "icon/icon128.png"
    }
  }